model emd {
  id               Int      @id @default(autoincrement())
  Date             DateTime
  VoucherType      String   @db.VarChar(191)
  Amount           Float?   @default(0.0)
  Type             String   @db.VarChar(191)
  URNNumber        String   @db.VarChar(191)
  StatusOfRefunded String   @db.VarChar(191)
  RefundedDate     DateTime
  NPNumbers        String   @db.VarChar(191)
  PartyName        String   @db.VarChar(191)
  NameOfWork       String   @db.VarChar(191)
  Section          String   @db.VarChar(191)
  Remarks          String   @db.VarChar(191)
}